	//@ 43,14 SAY "   -ProcRegua() - Ajusta tamanho da regua"
	//@ 53,14 SAY "   -IncProc() - Incrementa posicao da regua"
	ACTIVATE DIALOG oDlg5
	Return


Static Function OkProc()
	Close(oDlg5)
	Processa( {|| GeraArquivo() } )

